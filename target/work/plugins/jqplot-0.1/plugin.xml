<plugin name='jqplot' version='0.1' grailsVersion='1.3.5 &gt; *'>
  <author>Gert Wohlgemuth</author>
  <authorEmail>berlinguyinca@gmail.com</authorEmail>
  <title>JQPlot plugin</title>
  <description>\
This plugin provides an easy way to integrate the jqplot library into your project
</description>
  <documentation>http://grails.org/plugin/jqplot</documentation>
  <resources>
    <resource>BuildConfig</resource>
    <resource>Config</resource>
    <resource>DataSource</resource>
    <resource>UrlMappings</resource>
    <resource>jqplot.JqplotTagLib</resource>
  </resources>
  <dependencies>
    <plugin name='jquery' version='1.4.2.4 &gt; *' />
  </dependencies>
  <behavior />
</plugin>